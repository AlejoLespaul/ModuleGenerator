{
    "name": "alejo-lespaul/module-generator",
    "description": "A Module Generator for Laravel",
    "type": "library",
    "version": "1.2",
    "require": {
        "laravel/framework": "^8.12",
        "laminas/laminas-config": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "ModuleGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ModuleGenerator\\Test": "test/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ModuleGenerator\\Providers\\ModuleGeneratorProvider"
            ]
        }
    }
}
